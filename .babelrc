{
  "presets": ["env", "react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-regenerator",
    "transform-runtime",
    [
      "transform-imports",
      {
        "react-bootstrap": {
          "transform": "react-bootstrap/lib/${member}",
          "preventFullImport": true
        },
        "lodash": {
          "transform": "lodash/${member}",
          "preventFullImport": true
        }
      }
    ]
  ]
}
